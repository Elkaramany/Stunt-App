      ],
      "@Realm": [
        "./src/Realm"
      ],
      "@RealmTypes": [
        "./src/Realm/types.ts"
      ],
      "@RealmKeys": [
        "./src/Realm/keys.ts"
      ],
      "@Assets": [
        "./src/Assets"
      ],
    }
